[configuration]

entry_symbol = "library_init"
compatibility_minimum = "4.1"
reloadable = true

[libraries]

macos.debug = "bin/macos/libGodotKafka.framework"
macos.release = "bin/macos/libGodotKafka.framework"

ios.debug = "bin/ios/libGodotKafka.framework"
ios.release = "bin/ios/libGodotKafka.framework"

windows.debug.x86_32 = "bin/windows/libGodotKafka.dll"
