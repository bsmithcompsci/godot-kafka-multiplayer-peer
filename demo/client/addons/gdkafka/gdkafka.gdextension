linux.debug.x86_64 = "bin/linux/libGodotKafka.so"
linux.debug.arm64 = "bin/linux/libGodotKafka.so"
linux.debug.rv64 = "bin/linux/libGodotKafka.so"
linux.release.x86_64 = "bin/linux/libGodotKafka.so"
linux.release.arm64 = "bin/linux/libGodotKafka.so"
linux.release.rv64 = "bin/linux/libGodotKafka.so"

android.debug.x86_64 = "bin/android/libGodotKafka.so"
android.debug.arm64 = "bin/android/libGodotKafka.so"
